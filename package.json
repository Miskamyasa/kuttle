{
  "name": "kuttle-ui-app",
  "private": true,
  "repository": "https://github.com/kuttleio/kuttle-ui-app",
  "version": "0.0.1",
  "scripts": {
    "analyze": "npx vite-bundle-visualizer",
    "build": "tsc && ts-node build.ts && vite build",
    "dev": "NODE_ENV=development nodemon --config nodemon.json",
    "lint": "eslint -c .eslintrc ./frontend ./backend --fix",
    "postinstall": "npx patch-package --use-yarn && husky install",
    "preinstall": "npx only-allow yarn",
    "prepare": "husky install",
    "preview": "vite preview",
    "start": "CROSS_ENV=production node -r module-alias/register ./dist --env=production"
  },
  "engines": {
    "yarn": ">= 1.22",
    "node": ">=16.10.0"
  },
  "dependencies": {
    "@redux-devtools/extension": "3.2.3",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.15",
    "@types/fs-extra": "11.0.1",
    "@types/jsonwebtoken": "9.0.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.22",
    "@types/react-dom": "18.0.7",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "@vitejs/plugin-react": "2.2.0",
    "autoprefixer": "10.4.13",
    "axios": "1.2.2",
    "cookie-parser": "1.4.6",
    "dayjs": "1.11.7",
    "delay": "5.0.0",
    "dotenv": "16.0.3",
    "eslint": "8.31.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "express": "4.18.2",
    "express-async-errors": "3.1.1",
    "fs-extra": "11.1.0",
    "helmet": "6.0.1",
    "husky": "8.0.0",
    "jet-validator": "1.0.4",
    "jsonwebtoken": "9.0.0",
    "module-alias": "2.2.2",
    "nodemon": "2.0.20",
    "postcss": "8.4.20",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.6.2",
    "redux": "4.1.2",
    "redux-saga": "1.2.2",
    "reflect-metadata": "0.1.13",
    "sqlite3": "5.1.4",
    "tailwindcss": "3.2.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.4",
    "vite": "4.0.4"
  }
}
